#### Answer
if __name__ == '__main__':
    str1 = input('please input a string: ')
    str2 = input('please input a string: ')
    str3 = input('please input a string: ')
    print(str1, str2, str3)

    if str1 > str2:
        str1,str2 = str2, str1
    elif str1 > str3:
        str1, str3 = str3, str1
    elif str2 > str3:
        str2, str3 = str3, str2

    print('after being sorted')
    print(str1, str2, str3)
